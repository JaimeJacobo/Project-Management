jects/AddProject.js",[],["56","57"],"/Users/jaimejacobo/Desktop/project-management-client/src/components/projects/ProjectDetails.js",["58"],"import React, { Component } from 'react';\nimport axios from 'axios';\nimport { Link } from 'react-router-dom';\nimport EditProject from './EditProject';\nimport AddTask from '../tasks/AddTask';\n\nclass ProjectDetails extends Component {\n\tstate = {};\n\n\tcomponentDidMount() {\n\t\tthis.getSingleProject();\n\t}\n\n\tgetSingleProject = () => {\n\t\tconst { params } = this.props.match;\n\t\taxios\n\t\t\t.get(`https://project-management-back.herokuapp.com/api/projects/${params.id}`, { withCredentials: true })\n\t\t\t.then((responseFromApi) => {\n\t\t\t\tconst theProject = responseFromApi.data;\n\t\t\t\tthis.setState(theProject);\n\t\t\t})\n\t\t\t.catch((err) => {\n\t\t\t\tconsole.log(err);\n\t\t\t});\n\t};\n\n\trenderEditForm = () => {\n\t\tif (!this.state.title) {\n\t\t\tthis.getSingleProject();\n\t\t} else {\n\t\t\t//                                                    {...props} => so we can have 'this.props.history' in Edit.js\n\t\t\t//                                                                                          ^\n\t\t\t//                                                                                          |\n\t\t\treturn <EditProject theProject={this.state} getTheProject={this.getSingleProject} {...this.props} />;\n\t\t}\n\t};\n\n\t// DELETE PROJECT:\n\tdeleteProject = () => {\n\t\tconst { params } = this.props.match;\n\t\taxios\n\t\t\t.delete(`https://project-management-back.herokuapp.com/api/projects/${params.id}`, { withCredentials: true })\n\t\t\t.then(() => {\n\t\t\t\tthis.props.history.push('/projects'); // !!!\n\t\t\t})\n\t\t\t.catch((err) => {\n\t\t\t\tconsole.log(err);\n\t\t\t});\n\t};\n\n\trenderAddTaskForm = () => {\n\t\tif (!this.state.title) {\n\t\t\tthis.getSingleProject();\n\t\t} else {\n\t\t\t// pass the project and method getSingleProject() as a props down to AddTask component\n\t\t\treturn <AddTask theProject={this.state} getTheProject={this.getSingleProject} />;\n\t\t}\n\t};\n\n\townershipCheck = (project) => {\n\t\tif (this.props.loggedInUser && project.owner == this.props.loggedInUser._id) {\n\t\t\treturn (\n\t\t\t\t<div>\n\t\t\t\t\t<div>{this.renderEditForm()} </div>\n\t\t\t\t\t<button onClick={() => this.deleteProject(this.state._id)}>Delete project</button>\n\t\t\t\t</div>\n\t\t\t);\n\t\t}\n\t};\n\n\trender() {\n\t\treturn (\n\t\t\t<div>\n\t\t\t\t<h1>{this.state.title}</h1>\n\t\t\t\t<p>{this.state.description}</p>\n\t\t\t\t{/* show the task heading only if there are tasks */}\n\t\t\t\t{this.state.tasks && this.state.tasks.length > 0 && <h3>Tasks </h3>}\n\t\t\t\t{/* map through the array of tasks and... */}\n\t\t\t\t{this.state.tasks &&\n\t\t\t\t\tthis.state.tasks.map((task, index) => {\n\t\t\t\t\t\treturn (\n\t\t\t\t\t\t\t<div key={index}>\n\t\t\t\t\t\t\t\t{/* ... make each task's title a link that goes to the task details page */}\n\t\t\t\t\t\t\t\t<Link to={`/projects/${this.state._id}/tasks/${task._id}`}>{task.title}</Link>\n\t\t\t\t\t\t\t</div>\n\t\t\t\t\t\t);\n\t\t\t\t\t})}\n\t\t\t\t{this.ownershipCheck(this.state)}\n\t\t\t\t<br />\n\t\t\t\t<div>{this.renderAddTaskForm()} </div>\n\t\t\t\t<br />\n\t\t\t\t<br />\n\t\t\t\t<br />\n\t\t\t\t<br />\n\t\t\t\t<br />\n\t\t\t\t<Link to={'/projects'}>Back to projects</Link>\n\t\t\t</div>\n\t\t);\n\t}\n}\n\nexport default ProjectDetails;\n","/Users/jaimejacobo/Desktop/project-management-client/src/components/projects/ProjectList.js",[],"/Users/jaimejacobo/Desktop/project-management-client/src/components/navbar/Navbar.js",[],"/Users/jaimejacobo/Desktop/project-management-client/src/components/projects/EditProject.js",[],"/Users/jaimejacobo/Desktop/project-management-client/src/components/tasks/AddTask.js",[],"/Users/jaimejacobo/Desktop/project-management-client/src/components/tasks/TaskDetails.js",[],"/Users/jaimejacobo/Desktop/project-management-client/src/components/auth/Signup.js",[],"/Users/jaimejaco